  "description": "",
  "main": "src/index.js",
  "author": "Geno Roupsky <geno@roupsky.name> (https://github.com/groupsky)",
  "license": "ISC",
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.5",
    "budo": "^9.4.5",
    "cross-env": "^2.0.0",
    "envify": "^4.0.0",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "gh-pages": "^0.12.0",
    "less": "^2.7.2",
    "minifyify": "^7.3.4",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "standard": "^8.6.0",
    "uglify-js": "^2.7.5",
    "uglifyify": "^3.0.4",
    "vueify": "^9.4.0"
  },
  "dependencies": {
    "bootstrap-vue": "^0.16.1",
    "bulk-require": "^1.0.0",
    "bulkify": "^1.4.2",
    "cp-cli": "^1.0.2",
    "lodash": "^4.17.4",
    "lz-string": "^1.4.4",
    "moment": "^2.18.1",
    "raven-js": "^3.12.0",
    "vue": "^2.1.10",
    "vue-router": "^2.5.3"
  },
  "aliasify": {
    "aliases": {
      "vue": "vue/dist/vue.js"
    }
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      "bulkify"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "mkdirp build",
    "build": "npm run build:static && npm run build:js",
    "build:static": "cp-cli public build",
    "build:js": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o build/styles.css ] --debug -e src/index.js | uglifyjs -c warnings=false -m --screw-ie8 > build/scripts.js",
    "deploy": "gh-pages -d build -t",
    "dev": "budo src/index.js:scripts.js --live -d public --pushstate -- -p browserify-hmr",
    "lint": "eslint --ext .js,.vue src test/unit",
    "start": "budo src/index.js:scripts.js --live -d public --pushstate -- -t aliasify",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
